.row.catalog-wrapper
  .columns.large-12
    .row
      = render :partial => 'partials/catalog_sidebar'
      .columns.large-8
        .catalog-content-title-wrapper
          .line-wrapper
            .line
          .titles
            h3
              |Двері
            h4
              |розсувні двері-гармошки
          .image
            img src=asset_path("one-item-icon-after-title-content.jpg")
        .clearfix
        .catalog-content-wrapper
          .columns.image-block
            a href=@door_test.door_variants_fulfillment_options[0].image
              img src=@door_test.door_variants_fulfillment_options[0].image
          .columns.content-block
            h3 =@door_test.name
            p =raw @door_test.descriptions

            h5
              |Варіанти виконання:
            ul#variants
              li.box-shadow-right-bottom
                .thumbnail-item
                  a data-slide-index="0" href=""
                    img src=asset_path("dveri-1.jpg")
              li.box-shadow-right-bottom
                .thumbnail-item
                  a data-slide-index="1" href=""
                    img src=asset_path("dveri-3.jpg")
            /.columns#variant-tab-container.tab-container
            /  ul.varinty_vykon
            /    -@door_test.door_variants_fulfillment_options.each do |img_v|
            /      li.tab
            /        =link_to 't ',"#"+img_v.name

            .clearfix
            h5
              |Варіанти кольору:
            ul#variants-color
              li
                ul
                  li
                    a href=asset_path("dveri-1.jpg")
                      img src=asset_path("dveri-1.jpg")
                  li
                    a href=asset_path("dveri-2.jpg")
                      img src=asset_path("dveri-2.jpg")
              li
                ul
                  li
                    a href=asset_path("dveri-3.jpg")
                      img src=asset_path("dveri-3.jpg")
                  li
                    a href=asset_path("dveri-4.jpg")
                      img src=asset_path("dveri-4.jpg")
              //ul.varinty_color
              /-@door_test.door_variants_fulfillment_options.each do |img_v|
              /  -img_v.door_color_options.each do |img_v_c|
              /    ul id=img_v_c.name
              /      li
              /        a href=''
              /          img src=img_v_c.image

          .clearfix
          .row.door-info-table
