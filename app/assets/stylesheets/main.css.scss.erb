  border-bottom: 1px solid rgba(211, 220, 227, 0.35);
  transition: 0.6s ease-out all;
  -webkit-transition: 0.6s ease-out all;
}
.row.footer-bottom-content .columns.large-5 a:hover {
  border-bottom: 1px solid #d3dce3;
}
.footer-menu, .footer-contacts {
  padding-left: 6%;
}
.footer-contacts p {
  font-size: 15px;
  font-weight: 100;
  line-height: 23px;
}
.footer-contacts p span {
  font-size: 20px;
  font-weight: 500;
}
.columns.large-2.work-time {
  padding-left: 0;
}
.work-time p {
  font-size: 15px;
  font-weight: 100;
  line-height: 8px;
}
.work-time p span {
  float: right;
  font-size: 16px;
  font-weight: 500;
}
.columns.search-bar-background-left, .columns.search-bar-background-right {
  background: url("<%= asset_path('search-pannel-background-pattern.jpg') %>");
  height: 110px;
  width: 55px;
  position: absolute;
  top: 0px;
}
.columns.search-bar-background-left {
  left: -10px;
}
.columns.search-bar-background-right {
  right: -10px;
}
.bxslider li {
  left: -38px;
}
#bx-pager li {
  list-style: none;
  float: left;
  overflow: hidden;
  width: 115px;
  height: 90px;
  padding-left: 4%;
  padding-right: 4%;
  margin-top: 6px;
}
#bx-pager li img {
  width: 900px;
  position: relative;
  top: 10px;
  height: 100px;
}
.bx-viewport {
  height: 620px!important;
}
.main-menu-catalog {
  display: none;
  width: 750px;
  position: absolute;
  background: rgba(1, 1, 1, 0.8);
  top: 73px;
  left: -165%;
  border-left: 4px solid #00539c;
}
.sub-menu-links span a {
    border-bottom: 1px solid rgba(255,255,255,0.3);
    transition: 0.6s ease-out all;
    -webkit-transition: 0.6s ease-out all;
}
.interest-info h5 a, .helpful-articles h5 a {
    color: #005baa;
    line-height: 1.3em;
     border-bottom: 1px solid rgba(0,91,170,0.3);
     transition: 0.6s ease-out all;
     -webkit-transition: 0.6s ease-out all;
 }
.interest-info h5 a:hover, .helpful-articles h5 a:hover {
    border-bottom: 1px solid rgba(0,91,170,1);
}
.sub-menu-links span a:hover {
    border-bottom: 1px solid rgba(255,255,255,1);
}
.sub-menu-links span {
    display: block;
}
#main-menu li:hover ul {
  display: block;
}

.sub-menu-item h2 {
    color: white;
    font-size: 22px;
    font-weight: 500;
}
.active:after{
  bottom: 83%;
  left: 50%;
  border: solid transparent;
  content: " ";
  height: 0;
  width: 0;
  position: absolute;
  pointer-events: none;
  border-color: rgba(136, 183, 213, 0);
  border-bottom-color: #88b7d5;
  border-width: 14px;
  margin-left: -16px;
}
.row.catalog-wrapper, .row.content-wrapper {
  margin-top: -85px;
  padding-top: 85px;
  background: white;
    position: relative;
}
.bxslider, .bxslider li, .bxslider li img {
  height: 620px!important;
}
.active_menu {}
.row.content-wrapper {
  padding-bottom: 100px;
}
.catalog {
  /*padding: 27px 50px 21px 31px!important;*/
  padding: 27px 0px 21px 0px !important;
}
.catalog > a:after {
  width: 13px;
  height: 8px;
  display: block;
  background: url("<%= asset_path('Blue&grey-arrow.png') %>")no-repeat;
  content: '';
  position: absolute;
  right: 15px;
  top: 36px;
}
/*, #main-menu > .catalog:hover a:first-child:after*/
.active_menu:first-child:after {
  background-position: bottom;
}
.sub-menu-item {
  list-style: none;
  /*width: 33%;*/
  padding: 20px 17px 34px 34px;
  float: left;
}
.sub-menu-links a {
  color: white!important;
  font-weight: 100;
    font-size: 16px;
}
.page-content-wrapper-box-shadow {
    -webkit-box-shadow: -5px 0px 0px 0px rgba(80, 82, 84, 0.35), 5px 0px 0px 0px rgba(80, 82, 84, 0.35);
    -moz-box-shadow: -5px 0px 0px 0px rgba(80, 82, 84, 0.35), 5px 0px 0px 0px rgba(80, 82, 84, 0.35);
    box-shadow: -5px 0px 0px 0px rgba(80, 82, 84, 0.35), 5px 0px 0px 0px rgba(80, 82, 84, 0.35);
}
.sub-menu-item h2 img {
    padding-right: 15px;
}





