.designer-wrapper
    background: #999999
    padding:
        bottom: 100px
        top: 132px
    position: relative

#visual_designer
    height: 593px
    width: 1054px
    background: white
    margin-left: -27px
    position: relative
    overflow: hidden

    .vd_comp
        top: -38px

    .vd_nav
        left: -40px

    .vd_nav_ico
        position: absolute

        background: #9c9898 /* Old browsers */
        background: -moz-linear-gradient(top,  #9c9898 0%, #bdbebf 100%) /* FF3.6+ */
        background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#9c9898), color-stop(100%,#bdbebf)) /* Chrome,Safari4+ */
        background: -webkit-linear-gradient(top,  #9c9898 0%,#bdbebf 100%) /* Chrome10+,Safari5.1+ */
        background: -o-linear-gradient(top,  #9c9898 0%,#bdbebf 100%) /* Opera 11.10+ */
        background: -ms-linear-gradient(top,  #9c9898 0%,#bdbebf 100%) /* IE10+ */
        background: linear-gradient(to bottom,  #9c9898 0%,#bdbebf 100%) /* W3C */
        filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#9c9898', endColorstr='#bdbebf',GradientType=0 ) /* IE6-9 */

        cursor: pointer
        width: 40px
        height: 38px

        &:before
            display: block
            content: ""
            position: absolute
            top: 5px
            left: 5px
            width: 30px
            height: 28px
            border-top: 17px double #58595b
            border-bottom: 6px solid #58595b
        &.passive
            // needs latest Compass, add '@import "compass"' to your scss
            background: #fefefe /* Old browsers */
            background: -moz-linear-gradient(top,  #fefefe 0%, #bdbdbe 100%) /* FF3.6+ */
            background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#fefefe), color-stop(100%,#bdbdbe)) /* Chrome,Safari4+ */
            background: -webkit-linear-gradient(top,  #fefefe 0%,#bdbdbe 100%) /* Chrome10+,Safari5.1+ */
            background: -o-linear-gradient(top,  #fefefe 0%,#bdbdbe 100%) /* Opera 11.10+ */
            background: -ms-linear-gradient(top,  #fefefe 0%,#bdbdbe 100%) /* IE10+ */
            background: linear-gradient(to bottom,  #fefefe 0%,#bdbdbe 100%) /* W3C */
            filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#fefefe', endColorstr='#bdbdbe',GradientType=0 ) /* IE6-9 */

            &:before
                display: block
                content: ""
                position: absolute
                top: 5px
                left: 5px
                width: 30px
                height: 28px
                border-top: 17px double #004b8c
                border-bottom: 6px solid #004b8c
            &:hover
                background: #9c9898 /* Old browsers */
                background: -moz-linear-gradient(top,  #9c9898 0%, #bdbebf 100%) /* FF3.6+ */
                background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#9c9898), color-stop(100%,#bdbebf)) /* Chrome,Safari4+ */
                background: -webkit-linear-gradient(top,  #9c9898 0%,#bdbebf 100%) /* Chrome10+,Safari5.1+ */
                background: -o-linear-gradient(top,  #9c9898 0%,#bdbebf 100%) /* Opera 11.10+ */
                background: -ms-linear-gradient(top,  #9c9898 0%,#bdbebf 100%) /* IE10+ */
                background: linear-gradient(to bottom,  #9c9898 0%,#bdbebf 100%) /* W3C */
                filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#9c9898', endColorstr='#bdbebf',GradientType=0 ) /* IE6-9 */

                &:before
                    display: block
                    content: ""
                    position: absolute
                    top: 5px
                    left: 5px
                    width: 30px
                    height: 28px
                    border-top: 17px double #58595b
                    border-bottom: 6px solid #58595b

    .vd_nav_ico:hover
        // needs latest Compass, add '@import "compass"' to your scss
        background: #fefefe /* Old browsers */
        background: -moz-linear-gradient(top,  #fefefe 0%, #bdbdbe 100%) /* FF3.6+ */
        background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#fefefe), color-stop(100%,#bdbdbe)) /* Chrome,Safari4+ */
        background: -webkit-linear-gradient(top,  #fefefe 0%,#bdbdbe 100%) /* Chrome10+,Safari5.1+ */
        background: -o-linear-gradient(top,  #fefefe 0%,#bdbdbe 100%) /* Opera 11.10+ */
        background: -ms-linear-gradient(top,  #fefefe 0%,#bdbdbe 100%) /* IE10+ */
        background: linear-gradient(to bottom,  #fefefe 0%,#bdbdbe 100%) /* W3C */
        filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#fefefe', endColorstr='#bdbdbe',GradientType=0 ) /* IE6-9 */

        &:before
            display: block
            content: ""
            position: absolute
            top: 5px
            left: 5px
            width: 30px
            height: 28px
            border-top: 17px double #004b8c
            border-bottom: 6px solid #004b8c

    .vd_component_one_item
        position: relative
        float: left
        width: 130px
        height: 100%
        background: #ffffff /* Old browsers */
        background: -moz-linear-gradient(top,  #ffffff 1%, #bebdbe 100%) /* FF3.6+ */
        background: -webkit-gradient(linear, left top, left bottom, color-stop(1%,#ffffff), color-stop(100%,#bebdbe)) /* Chrome,Safari4+ */
        background: -webkit-linear-gradient(top,  #ffffff 1%,#bebdbe 100%) /* Chrome10+,Safari5.1+ */
        background: -o-linear-gradient(top,  #ffffff 1%,#bebdbe 100%) /* Opera 11.10+ */
        background: -ms-linear-gradient(top,  #ffffff 1%,#bebdbe 100%) /* IE10+ */
        background: linear-gradient(to bottom,  #ffffff 1%,#bebdbe 100%) /* W3C */
        filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#bebdbe',GradientType=0 ) /* IE6-9 */
        border-right: 1px solid #8a9095
        text-align: center
        color: #004b8c
        font-size: 0.875rem
        text-transform: uppercase
        font-weight: 600
        cursor: pointer

        &:hover
            background: #9c9898 /* Old browsers */
            background: -moz-linear-gradient(top,  #9c9898 0%, #bdbebf 100%) /* FF3.6+ */
            background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#9c9898), color-stop(100%,#bdbebf)) /* Chrome,Safari4+ */
            background: -webkit-linear-gradient(top,  #9c9898 0%,#bdbebf 100%) /* Chrome10+,Safari5.1+ */
            background: -o-linear-gradient(top,  #9c9898 0%,#bdbebf 100%) /* Opera 11.10+ */
            background: -ms-linear-gradient(top,  #9c9898 0%,#bdbebf 100%) /* IE10+ */
            background: linear-gradient(to bottom,  #9c9898 0%,#bdbebf 100%) /* W3C */
            filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#9c9898', endColorstr='#bdbebf',GradientType=0 ) /* IE6-9 */
            color: #58595b

    .vd_render_interior
        width: 229px

        .vd_image
            position: absolute
            width: 100%
            height: 100%
            -webkit-background-size: cover !important
            -moz-background-size: cover !important
            -o-background-size: cover !important
            background-size: cover !important

        .vd_render_interior_title
            position: absolute
            background: rgba(61, 61, 61, 0.7)
            color: #ffffff
            width: 100%
            height: 100%

            svg
                fill: #ffffff
                width: 60px



#vd_view
    height: 100%
    width: 100%

#vd_components
    width: 749px
    height: 140px
    position: absolute
    left: 0
    bottom: 0

#vd_door_nav
    width: 305px
    height: 100%
    background: url("<%= asset_path('sb-metall-bg-512px.jpg') %>")
    position: absolute
    top: 0
    right: 0
    bottom: 0
    -webkit-transition: right 0.5s ease-out
    -moz-transition: right 0.5s ease-out
    -o-transition: right 0.5s ease-out
    transition: right 0.5s ease-out

#vd_door_nav.right_null
    right: -305px !important

#visual_constructor
    width: 1099px
    height: 750px
    background: lawngreen
    position: relative
    left: -7%
    z-index: 100

    #vc_navigation
        width: 468px
        height: 100%
        background: darkred
        position: absolute
        right: 0
        top: 0

        .vcc_backgrounds
            width: 183px

            img
                width: 183px